{
    "manifest_version": 3,
    "name": "Tigran's clunky extension",
    "description": "add a custom background to google.com",
    "version": "0.1",
    "icons": {
        "16": "./icons/icon16.png",
        "48": "./icons/icon48.png",
        "128": "./icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://relay.amazon.com/loadboard/search"
            ],
            "js": [
                "click.js"
            ]
        }
    ]
}